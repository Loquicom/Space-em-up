    vspeed = lengthdir_y(vitP2, -135);
}
else if(keyboard_check(vk_down) &amp;&amp; keyboard_check(vk_right)){
    hspeed = lengthdir_x(vitP2, -45);
    vspeed = lengthdir_y(vitP2, -45);
}
//Test les combinaisons de touches unidirection (1 touche)
else if(keyboard_check(vk_up)){
    hspeed = 0;
    vspeed = -vitP2;
}
else if(keyboard_check(vk_left)){
    hspeed = -vitP2;
    vspeed = 0;
}
else if(keyboard_check(vk_down)){
    hspeed = 0;
    vspeed = vitP2;
}
else if(keyboard_check(vk_right)){
    hspeed = vitP2;
    vspeed = 0;
}
//Toute les autre combinaisons de touches
else{
    hspeed = 0;
    vspeed = 0;
}

