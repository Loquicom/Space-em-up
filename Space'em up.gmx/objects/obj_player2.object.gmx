    hspeed = lengthdir_x(vitP2, 135);
    vspeed = lengthdir_y(vitP2, 135);
}
else if(keyboard_check(vk_up) &amp;&amp; keyboard_check(vk_right)){
    hspeed = lengthdir_x(vitP2, 45);
    vspeed = lengthdir_y(vitP2, 45);
}
