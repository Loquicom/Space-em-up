          <argument>
            <kind>1</kind>
            <string>///Déplacement de l'objet clavier

if(!pause){

//Si le jeu n'est pas en tactile
if(!tactile){
    //Test les combinaisons de touches unidirection(3 touches)
    if(keyboard_check(ord('Z')) &amp;&amp; keyboard_check(ord('Q')) &amp;&amp; keyboard_check(ord('D'))){
        hspeed = 0;
        vspeed = -vitP1;
    }
    else if(keyboard_check(ord('Q')) &amp;&amp; keyboard_check(ord('Z')) &amp;&amp; keyboard_check(ord('S'))){
        hspeed = -vitP1;
        vspeed = 0;
    }
    else if(keyboard_check(ord('S')) &amp;&amp; keyboard_check(ord('Q')) &amp;&amp; keyboard_check(ord('D'))){
        hspeed = 0;
        vspeed = vitP1;
    }
    else if(keyboard_check(ord('D')) &amp;&amp; keyboard_check(ord('Z')) &amp;&amp; keyboard_check(ord('S'))){
        hspeed = vitP1;
        vspeed = 0;
    }
    //Test les combinaisons de touches diagonale (2 touches)
    else if(keyboard_check(ord('Z')) &amp;&amp; keyboard_check(ord('Q'))){
        hspeed = lengthdir_x(vitP1, 135);
        vspeed = lengthdir_y(vitP1, 135);
    }
    else if(keyboard_check(ord('Z')) &amp;&amp; keyboard_check(ord('D'))){
        hspeed = lengthdir_x(vitP1, 45);
        vspeed = lengthdir_y(vitP1, 45);
    }
    else if(keyboard_check(ord('S')) &amp;&amp; keyboard_check(ord('Q'))){
        hspeed = lengthdir_x(vitP1, -135);
        vspeed = lengthdir_y(vitP1, -135);
    }
    else if(keyboard_check(ord('S')) &amp;&amp; keyboard_check(ord('D'))){
        hspeed = lengthdir_x(vitP1, -45);
        vspeed = lengthdir_y(vitP1, -45);;
    }
    //Test les combinaisons de touches unidirection (1 touche)
    else if(keyboard_check(ord('Z'))){
        hspeed = 0;
        vspeed = -vitP1;
    }
    else if(keyboard_check(ord('Q'))){
        hspeed = -vitP1;
        vspeed = 0;
    }
    else if(keyboard_check(ord('S'))){
        hspeed = 0;
        vspeed = vitP1;
    }
    else if(keyboard_check(ord('D'))){
        hspeed = vitP1;
        vspeed = 0;
    }
    //Toute les autre combinaisons de touches
    else{
        hspeed = 0;
        vspeed = 0;
    }
}

}
